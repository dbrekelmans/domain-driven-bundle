{
    "name": "dbrekelmans/domain-driven-bundle",
    "description": "Domain driven directory structure configuration for Symfony.",
    "authors": [
        {
            "name": "DaniÃ«l Brekelmans",
            "homepage": "https://github.com/dbrekelmans"
        }
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dbrekelmans/domain-driven-bundle/issues"
    },
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1",
        "dbrekelmans/coding-standard": "^1.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/routing": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "symfony/framework-bundle": "Automatically configure services and routes for each domain context.",
        "symfony/twig-bundle": "Automatically configure a template path per domain context."
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "DomainDrivenBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
